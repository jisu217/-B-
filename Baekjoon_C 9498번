#include <stdio.h>

int main(void) {
    int score;
    scanf("%d", &score); // &를 추가하여 score의 주소를 전달합니다.

    if (score >= 90 && score <= 100) { // 논리 연산자 &&를 사용하여 범위를 지정합니다.
        printf("A");
    } else if (score >= 80 && score <= 89) {
        printf("B");
    } else if (score >= 70 && score <= 79) {
        printf("C");
    } else if (score >= 60 && score <= 69) {
        printf("D");
    } else {
        printf("F");
    }

    return 0;
}
